pipeline {
    agent any
    stages {
        stage('Docker: Build') {
            steps {
                dir('testing_folder') {
                    sh "docker-compose -f docker-compose-lambda.yml up --build"
                }
            }
        }
        stage('Docker: Quit') {
            steps {
                dir('testing_folder') {
                    sh "docker-compose down"
                }
            }
        }
    }
}